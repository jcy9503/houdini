nd"], "role": "item", "extent": [2264, 2294], "container": true, "type": "parameters_item"}, {"body": [{"indent": 23, "type": "para", "extent": [2403, 2473], "text": ["Pushing the end points away from other points."]}], "indent": 12, "text": ["Push End"], "role": "item", "extent": [2381, 2403], "container": true, "type": "parameters_item"}, {"body": [{"indent": 23, "type": "para", "extent": [2499, 2587], "text": ["When pushing the end points this value will control the falloff."]}], "indent": 12, "text": ["Push Falloff"], "role": "item", "extent": [2473, 2499], "container": true, "type": "parameters_item"}], "container": true, "role": "item_group", "type": "parameters_item_group"}], "indent": 8, "level": 3, "text": ["Contact Spread"], "extent": [825, 854], "container": true, "type": "h", "id": null}, {"body": [{"body": [{"body": [{"indent": 16, "type": "para", "extent": [2640, 2722], "text": ["Option to control what information is stored in the vertex color."]}], "indent": 12, "text": ["Red Channel"], "role": "item", "extent": [2615, 2640], "container": true, "type": "parameters_item"}, {"body": [{"indent": 16, "type": "para", "extent": [2749, 2831], "text": ["Option to control what information is stored in the vertex color."]}], "indent": 12, "text": ["Green Channel"], "role": "item", "extent": [2722, 2749], "container": true, "type": "parameters_item"}, {"body": [{"indent": 16, "type": "para", "extent": [2857, 2939], "text": ["Option to control what information is stored in the vertex color."]}], "indent": 12, "text": ["Blue Channel"], "role": "item", "extent": [2831, 2857], "container": true, "type": "parameters_item"}], "container": true, "role": "item_group", "type": "parameters_item_group"}], "indent": 8, "level": 3, "text": ["Vertex Colors"], "extent": [2587, 2615], "container": true, "type": "h", "id": null}], "indent": 4, "level": 2, "text": ["Settings"], "extent": [534, 553], "container": true, "type": "h", "id": null}, {"body": [{"body": [{"body": [{"inde